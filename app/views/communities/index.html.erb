<h1>Community#index</h1>
<p>Find me in app/views/community/index.html.erb</p>

<h1>LIST ALL COMMUNITY POSTS</h1>

<% if @communities %>
	<ul>
		<% @communities.reverse.each do |community| %>
			<li>
			<%= community.title %>
			<%= community.text %>
			<a href ="#">COMMENT</a>
			</li>
		<% end %>
	</ul>
<% end %>

<h3>render posts#show aka GET /posts/:id/ </h3>
<%= render "comments/new" %>